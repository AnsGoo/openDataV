    "pinia": "^2.0.0-rc.10",
    "vite-plugin-mock": "^2.9.6",
    "vue": "^3.2.37",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/codemirror": "^5.60.5",
