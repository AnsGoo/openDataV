{
  "name": "open-data-v",
  "version": "0.0.0",
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "tsup --watch",
    "demo": "npm -C examples run dev",
    "build:demo": "npm -C examples run build",
    "build": "gulp --require @esbuild-kit/cjs-loader -f build/index.ts",
    "lint": "eslint --ext .js,.ts,.vue,.tsx packages/ examples/src docs/ resource/",
    "lint:fromat": "prettier -c --parser typescript \"{packages,__tests__,e2e}/**/*.[jt]s?(x)\"",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue,.tsx packages/ examples/ docs/ resource/",
    "prepare": "husky install",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf packages/**/node_modules && rimraf node_modules && pnpm bootstrap"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "rollup-plugin-typescript2": "^0.36.0",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/gulp": "^4.0.17",
    "@types/node": "^16.18.61",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.17",
    "esbuild": "^0.20.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-spellcheck": "^0.0.20",
    "eslint-plugin-vue": "^9.18.1",
    "execa": "^8.0.1",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "less": "^4.2.0",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "rollup": "^4.10.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "^4.9.5",
    "unplugin-vue-define-options": "^1.4.2",
    "vite": "^4.5.2",
    "vue-tsc": "^1.8.22"
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "semi": false,
    "endOfLine": "lf"
  }
}
