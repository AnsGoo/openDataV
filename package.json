    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.16",
    "postcss-html": "^1.2.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.4.1",
